  ∀ (C : PointedCovering Y y0),
    IsUniversalCovering C ↔ ∀ (D : PointedCovering Y y0), ∃! (f : C.X → D.X), D.map ∘ f = C.map ∧ f C.point = D.point :=
  by sorry
