/-- The type of bounded continuous real-valued functions on a metric space X. -/
def C_b (X : Type*) [MetricSpace X] : Type* := { f : X → ℝ // Continuous f ∧ Bounded (Set.range f) }

namespace C_b

variable {X : Type*} [MetricSpace X]

