  IsUniversalCovering p ↔
    ∀ {Z : Type*} [TopologicalSpace Z] (q : PointedCoveringMap Z Y),
      ∃! (f : X → Z), (q.toFun ∘ f = p.toFun) ∧ (f p.x0 = q.x0) := by
  sorry
